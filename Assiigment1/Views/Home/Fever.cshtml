@{
    ViewData["Title"] = "Temperature";
}
<h2>Fever check</h2>
<form asp-controller="Home" asp-action="Fcheck" method="post">
    <input name="temperatur" id="Temperatur" type="number" min="34" max="41" placeholder="Temperatur" />
    <input name="choise" id="choise" type="radio" value="Celcius" />Celcius
    <input name="choise" id="choise" type="radio" value="Farenhait" />Farenhait
    <button>Fever check</button>
</form>

    <h3>@ViewBag.DoctorSays</h3>

